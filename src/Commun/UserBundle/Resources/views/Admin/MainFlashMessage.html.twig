{% for flash_message in app.session.flashbag.get('notice') %}

        <div class="alert alert-success alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true"><i
                        class="fa fa-times-circle"></i>
            </button>
            <p>{{ flash_message }}</p>
        </div>
{% endfor %}