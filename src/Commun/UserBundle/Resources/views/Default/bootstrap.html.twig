<!DOCTYPE html>

<head>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}{% if pageTitle is defined%}{{ pageTitle }}{% endif %}{% endblock %} - Front</title>

    <script src="{{ asset('assets/modernizr/modernizr.custom.js') }}"></script>
    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css')}}">
    <link rel="stylesheet" href="{{ asset('css/entypo.css')}}">
    <link rel="stylesheet" href="{{ asset('css/font-awesome.css')}}">
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/domotiquebundlefront/images/favicon.ico') }}" />
<body>
    {% block nav %}{% include'CommunUserBundle:Default:nav.html.twig' %}{% endblock %}
    {% block body %}{% endblock %}

    {#<script src="{{ asset('assets/jquery/jquery.min.js') }}"></script>#}
    {#<script src="{{ asset('assets/jquery-ui/jquery-ui.min.js') }}"></script>#}


    <!--jquery-->
    {% javascripts '@CommunUserBundle/Resources/public/js/jquery/jquery.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <!--jquery-ui-->
    {% javascripts '@CommunUserBundle/Resources/public/js/jquery/jquery-ui.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <!--Bootstrap-->
    {% javascripts '@CommunUserBundle/Resources/public/js/bootstrap/bootstrap.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% block javascripts %}{% endblock %}
</body>

</head>
</html>