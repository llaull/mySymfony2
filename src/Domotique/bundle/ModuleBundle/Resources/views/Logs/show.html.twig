{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Logs</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Sondevaleur</th>
                <td>{{ entity.sondeValeur }}</td>
            </tr>
            <tr>
                <th>Sondeid</th>
                <td>{{ entity.sondeId }}</td>
            </tr>
            <tr>
                <th>Temps</th>
                <td>{{ entity.temps|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_domotique_module_logs') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
