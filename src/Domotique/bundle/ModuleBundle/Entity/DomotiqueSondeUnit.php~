<?php

namespace Domotique\bundle\ModuleBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * DomotiqueSondeUnit
 *
 * @ORM\Table(name="domotique__sonde_unit")
 * @ORM\Entity
 */
class DomotiqueSondeUnit
{
    /**
     * @var integer
     *
     * @ORM\Column(name="unit_ID", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $unitId;

    /**
     * @var string
     *
     * @ORM\Column(name="symbole", type="text", length=65535, nullable=false)
     */
    private $symbole;

    /**
     * @var string
     *
     * @ORM\Column(name="nom", type="text", length=65535, nullable=false)
     */
    private $nom;

    /**
     * @var string
     *
     * @ORM\Column(name="nom_lien", type="text", length=65535, nullable=false)
     */
    private $nomLien;


}

