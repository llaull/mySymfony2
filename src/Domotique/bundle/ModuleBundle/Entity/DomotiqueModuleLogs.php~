<?php

namespace Domotique\bundle\ModuleBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * DomotiqueModuleLogs
 *
 * @ORM\Table(name="domotique__module_logs", indexes={@ORM\Index(name="modules_id", columns={"modules_id"}), @ORM\Index(name="sonde_type", columns={"sonde_type"}), @ORM\Index(name="sonde_unit", columns={"sonde_unit"})})
 * @ORM\Entity
 */
class DomotiqueModuleLogs
{
    /**
     * @var integer
     *
     * @ORM\Column(name="log_ID", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $logId;

    /**
     * @var string
     *
     * @ORM\Column(name="sonde_valeur", type="decimal", precision=10, scale=2, nullable=false)
     */
    private $sondeValeur;

    /**
     * @var integer
     *
     * @ORM\Column(name="sonde_id", type="integer", nullable=true)
     */
    private $sondeId;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="temps", type="datetime", nullable=false)
     */
    private $temps;

    /**
     * @var \Domotique\bundle\ModuleBundle\Entity\DomotiqueModuleInfos
     *
     * @ORM\ManyToOne(targetEntity="Domotique\bundle\ModuleBundle\Entity\DomotiqueModuleInfos")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="modules_id", referencedColumnName="modules_ID")
     * })
     */
    private $modules;

    /**
     * @var \Domotique\bundle\ModuleBundle\Entity\DomotiqueSondeType
     *
     * @ORM\ManyToOne(targetEntity="Domotique\bundle\ModuleBundle\Entity\DomotiqueSondeType")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="sonde_type", referencedColumnName="type_ID")
     * })
     */
    private $sondeType;

    /**
     * @var \Domotique\bundle\ModuleBundle\Entity\DomotiqueSondeUnit
     *
     * @ORM\ManyToOne(targetEntity="Domotique\bundle\ModuleBundle\Entity\DomotiqueSondeUnit")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="sonde_unit", referencedColumnName="unit_ID")
     * })
     */
    private $sondeUnit;


}

