{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Log</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Sonsorvalue</th>
                <td>{{ entity.sonsorValue }}</td>
            </tr>
            <tr>
                <th>Sensorid</th>
                <td>{{ entity.sensorId }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_sensor_log') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_sensor_log_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
