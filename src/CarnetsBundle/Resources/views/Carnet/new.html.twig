{% extends "CommunUserBundle:Admin:MainWrapper.html.twig" %}
{% block title %}les carnets de voyages{% endblock title %}
{% block h1_header %}{% endblock h1_header %}
{% block h1_header_s %}-{% endblock h1_header_s %}
{% block controlerLink %}{{ path('admin_carnet') }}{% endblock controlerLink %}
{% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}
{% trans_default_domain "app" %}
{% block dynamic_body %}

    <div class="col-md-12 bootstrap-grid">

        <div class="powerwidget" id="datatable-basic-init" data-widget-editbutton="false">
            <header>
                <h2>new
                    <small>emplacement de module</small>
                </h2>
            </header>
            <div class="panel-body">
                <div class="callout callout-info">
                    <h4>Informations</h4>

                    <p>le titre du carnet est limité à 19 caractéres</p>
                </div>
            </div>

            <div class="inner-spacer">
                <div class="inner-spacer">
                    {{ form(form) }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
